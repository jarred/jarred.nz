---
import Hr from './Hr.astro';
export interface Props {
	projects: [];
}
const {projects} = Astro.props;
---

<nav class="whitespace-pre">
<Hr />
  Navigation:
  -> <a class="" rel="prefetch" href="/">Index</a>
  -> <a class="" rel="prefetch" href="/about">About</a>

<Hr />  
Projects:
  {projects.map(project => <a href={project.url} class="flex flex-row">
      <div class="whitespace-pre">  -> </div>
      <div>{project.frontmatter.year}</div>
      <div>*</div>
      <div>{project.frontmatter.title}</div>
  </a>)}
  <Hr />  
</nav>